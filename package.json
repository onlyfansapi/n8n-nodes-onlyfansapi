{
  "name": "@onlyfansapi/n8n-nodes-onlyfansapi",
  "version": "1.0.0",
  "description": "n8n community node to work with OnlyFans API",
  "license": "MIT",
  "homepage": "https://onlyfansapi.com",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "OnlyFans API Team",
    "email": "tech@onlyfansapi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onlyfansapi/n8n-nodes-onlyfansapi"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OnlyFansApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OnlyFans/OnlyFans.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "0.11.0",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
